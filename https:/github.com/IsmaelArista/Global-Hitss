        "# 1) Conseguir el .ipynb en /content (o subirlo como plan B)\n",
        "import json\n",
        "\n",
        "src_nb = None\n",
        "try:\n",
        "    from google.colab import _message\n",
        "    resp = _message.blocking_request('get_ipynb', {})\n",
        "    nb = resp.get('ipynb') or resp.get('notebook') or None\n",
        "    if not nb:\n",
        "        raise RuntimeError(\"Colab no devolvió el JSON del notebook.\")\n",
        "    nb_name = nb.get('metadata', {}).get('colab', {}).get('name', 'notebook.ipynb')\n",
        "    with open(f'/content/{nb_name}', 'w') as f:\n",
        "        json.dump(nb, f)\n",
        "    src_nb = f'/content/{nb_name}'\n",
        "    print(\"Notebook guardado en:\", src_nb)\n",
        "except Exception as e:\n",
        "    print(\"No pude capturar el notebook actual:\", e)\n",
        "    print(\"Plan B: sube el .ipynb que quieres pushear…\")\n",
        "    from google.colab import files\n",
        "    up = files.upload()  # selecciona tu .ipynb\n",
        "    # toma el primer archivo subido\n",
        "    src_nb = '/content/' + next(iter(up.keys()))\n",
        "    print(\"Usando:\", src_nb)\n"
